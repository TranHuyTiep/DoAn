    <script>

        window.onload = function () {
            var socket = io();
//            socket.emit('login','')
//            socket.on('cpu_load',function (data) {
//                console.log(data)
//            })
                var dps = []; // dataPoints

                var chart = new CanvasJS.Chart("chartContainer", {
//              exportEnabled: true,
                    title: {},
                    axisY: {
                        includeZero: false,
                        maximum: 100,
                        minimum: 0,
                        title: "Nhiệt độ(C)"
//                  crosshair: {
//                      enabled: true,
//                      snapToDataPoint: true
//                  }

                    },
                    axisX: {
                        viewportMinimum: 0,
                        viewportMaximum: 60,
                        valueFormatString: "",
                        title: "Secondary"
//                  crosshair: {
//                      enabled: true,
//                      snapToDataPoint: true
//                  }

                    },
                    data: [{
                        type: "area",
                        dataPoints: dps
                    }]
                });

                var xVal = 0;
                var yVal = 100;
                var updateInterval = 1000;
                var dataLength = 60; // number of dataPoints visible at any point
                socket.on('cpu_load', function (data) {
                    dps.push({
                        x: xVal,
                        y: data.data
                    });
                    xVal++;
                    if (dps.length > dataLength) {
                        var viewportMinimum = (chart.axisX[0].viewportMinimum) + 1
                        var viewportMaximum = (chart.axisX[0].viewportMaximum) + 1
                        chart.axisX[0].set('viewportMinimum', viewportMinimum, true);
                        chart.axisX[0].set('viewportMaximum', viewportMaximum, true);
                        dps.shift();
                    }
                    chart.render();
                })
        }
    </script>

